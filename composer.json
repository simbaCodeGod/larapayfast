{
  "name": "simbasara/payfast",
  "authors": [
    {
      "name": "Simba Sara"
    }
  ],
  "description": "Laravel 9+ package for processing ITN payments through payfast.co.za",
  "keywords": ["package", "laravel", "payfast"],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^9.0",
    "illuminate/http": "^9.0",
    "billowapp/show-me-the-money": "^0.4"
  },
  "autoload": {
    "psr-4": {
      "Billow\\": "src/"
    }
  },
  "require-dev": {
    "symfony/var-dumper": "^3.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Billow\\PayfastServiceProvider"
      ]
    }
  }
}
